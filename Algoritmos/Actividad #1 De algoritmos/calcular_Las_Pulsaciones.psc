Proceso calcular_Las_Pulsaciones
	//Definir las variables
	definir edad, num_pulsaciones como real;
	//Datos De Entrada
	Escribir "Ingrese tu edad";
	leer edad;
	//procesos
	num_pulsaciones <- (2020-edad)/10;
	//Datos De salida
	EScribir "el numero de latidos por pulsacion en 10 segundoes es:",num_pulsaciones;;
	
FinProceso
