Proceso Calcular_la_Hipotenusa
	//Definir Variables 
	Definir cateto1, cateto2,hipotenusa Como Real;
	//Datos De entrada
	Escribir "ingrese el valor del cateto uno:";
	leer cateto1;
	Escribir  "Ingrese el valor del cateto dos:";
	Leer cateto2;
	//Procesos
	hipotenusa<- (cateto1*2)*(cateto2*2);
	//Datos De Salida
	Escribir "La hipotenusa es:", hipotenusa;
	
